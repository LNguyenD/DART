    <DataSet Name="ds_CPR_Weekly_Open">
      <Query>
        <DataSourceName>DART</DataSourceName>
        <CommandText>="exec usp_CPR_Last_Year_Monthly "
	&amp; "'" &amp; Parameters!System.Value &amp; "',"
	&amp; "'" &amp; iif(LCASE(Parameters!Type.Value) = "broker","broker",iif(LCASE(Parameters!Value.Value) = "all", Parameters!System.Value,
				   iif(Parameters!SubValue.Value = "all", Parameters!Type.Value,"team"))) &amp; "',"
	&amp; "'" &amp; iif(LCASE(Parameters!Type.Value) = "broker","broker",iif(LCASE(Parameters!Value.Value) = "all", Parameters!System.Value,
				   iif(Parameters!SubValue.Value = "all", REPLACE(Parameters!Value.Value,"'", "''''"),REPLACE(Parameters!SubValue.Value,"'", "''''")))) &amp; "',"
	&amp; "'" &amp; iif(LCASE(Parameters!Type.Value) = "broker","broker",iif(LCASE(Parameters!Value.Value) = "all", Parameters!System.Value,
				   iif(Parameters!SubValue.Value = "all", REPLACE(Parameters!Value.Value,"'", "''''"),REPLACE(Parameters!Value.Value,"'", "''''")))) &amp; "'"</CommandText>
      </Query>
