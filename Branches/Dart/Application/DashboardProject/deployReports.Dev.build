<?xml version="1.0" encoding="utf-8" ?>
<project name="ReportingProject" default="deploy" xmlns="http://nant.sf.net/release/0.91/nant.xsd">
  <property name="localReportsFolderName" value="ReportingProject"/>
  <property name="localReportsFolderPath" value="${path::get-full-path(localReportsFolderName)}"/>
  <target name="deploy" description="deploy reports">
    <exec program="SSRSDeployTool.Console.exe" >
      <arg value="https://dvnapp07.dvnaswig.local/Reportserver/reportservice2010.asmx" />
      <arg value="dbtest" />
      <arg value="abc" />
      <arg value="Data Source=10.9.11.13;Initial Catalog=Dart;Persist Security Info=True;User ID=dart;Password=password1" />
      <arg value="${localReportsFolderPath}" />
	  <!-- DashboardType|CacheHour|CacheMinute|RefreshHour|RefreshMinute|SnapshotHour|SnapshotMinute|TakeSnapshot -->
	  <arg value="AWC|5|0|5|5|6|30|true" />
	  <arg value="RTW|5|0|5|5|6|30|true" />
	  <arg value="CPR|4|0|4|5|6|30|true" />
	  <arg value="Others|4|0|4|5|6|30|true" />
    </exec>
  </target>
</project>
