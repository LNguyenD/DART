        </Field>
        <Field Name="Order">
          <Value>=iif((LCASE(RTRIM(Parameters!Value.Value)) = "all" and (LCASE(RTRIM(Fields!Value.Value)) = "tmf_total" or LCASE(RTRIM(Fields!Value.Value)) = "wcnsw_total" or LCASE(RTRIM(Fields!Value.Value)) = "hospitality_total"))
	or 
	(LCASE(RTRIM(Parameters!Value.Value)) &lt;&gt; "all" and LCASE(RTRIM(Parameters!SubValue.Value)) = "all" and LCASE(RTRIM(Parameters!Value.Value)) &amp; "_total" = LCASE(RTRIM(Fields!Value.Value)))
	or 
	(LCASE(RTRIM(Parameters!Value.Value)) &lt;&gt; "all" and LCASE(RTRIM(Parameters!SubValue.Value)) &lt;&gt; "all" and LCASE(RTRIM(Parameters!SubValue.Value)) &amp; "_total" = LCASE(RTRIM(Fields!Value.Value)))
	,2
	,iif(LCASE(RTRIM(Fields!Value.Value)) = "miscellaneous",1,0))</Value>
        </Field>
        <Field Name="Parents">
          <Value>=Switch(RTRIM(Fields!Value.Value) = "HEALTH", "HEALTH &amp; OTHER",
		RTRIM(Fields!Value.Value) = "OTHER", "HEALTH &amp; OTHER",
		RTRIM(Fields!Value.Value) = "POLICE", "POLICE &amp; FIRE",
		RTRIM(Fields!Value.Value) = "FIRE", "POLICE &amp; FIRE",
		Fields!Value.Value = "Accommodation", "Hotel",
		Fields!Value.Value = "Pubs, Taverns and Bars", "Hotel")</Value>
        </Field>
