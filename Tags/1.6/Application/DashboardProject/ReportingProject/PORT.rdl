        <CommandText>="exec usp_PORT_Summary " &amp; 
	"'" &amp; LCASE(RTRIM(Parameters!System.Value)) &amp; "'," &amp;
	"'" &amp; LCASE(RTRIM(Parameters!Type.Value)) &amp; "'," &amp;
	"'" &amp; LCASE(RTRIM(Parameters!Value.Value)) &amp; "'," &amp;
