	"'" &amp; LCASE(RTRIM(Parameters!Start_Date.Value)) &amp; "'," &amp;
	"'" &amp; LCASE(RTRIM(Parameters!End_Date.Value)) &amp; "'," &amp;
	"'" &amp; LCASE(RTRIM(Parameters!Claim_Liability_Indicator.Value)) &amp; "'," &amp;
	"'" &amp; LCASE(RTRIM(Parameters!Psychological_Claims.Value)) &amp; "'," &amp;
	"'" &amp; LCASE(RTRIM(Parameters!Inactive_Claims.Value)) &amp; "'," &amp;
	"'" &amp; LCASE(RTRIM(Parameters!Medically_Discharged.Value)) &amp; "'," &amp;
	"'" &amp; LCASE(RTRIM(Parameters!Exempt_From_Reform.Value)) &amp; "'," &amp;
	"'" &amp; LCASE(RTRIM(Parameters!Reactivation.Value)) &amp;	"'"</CommandText>
      </Query>
