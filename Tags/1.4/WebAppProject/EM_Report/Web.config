		<add key="No_Days_Blocked_Attempts" value="1" />
		<!--site settings-->
		<add key="usertypes" value="System,Internal,External" />
		<add key="pagesizes" value="10,20,50" />
		<add key="Top_Favorite" value="3" />
		<add key="Top_Recently" value="3" />
		<add key="Top_Category" value="3" />

		<add key="LoadingReportTimeout" value="180000" />
		<!-- 3*60*1000 -->
		<add key="IsNeedByPassCertificate" value="true" />
		<add key="UnwantedExportFormats" value="IMAGE,RPL,XML,MHTML" />
		<add key="ReportServerFormatDate" value="yyyy-MM-dd" />
		<add key="ReportWithNoWordExport" value="4.12.TOOCSCoding" />
		<!--site settings-->
		<add key="HelpFile" value="/dart/Datas/Pilot_DB_Training_Pack.ppt" />
		<add key="Cache_Dataset_At_Hour" value="6" />
		<add key="Cache_Dataset_At_Minute" value="0" />
		<add key="Refresh_Cache_Dataset_At_Hour" value="7" />
		<add key="Refresh_Cache_Dataset_At_Minute" value="0" />
		<add key="Snapshot_At_Hour" value="7" />
