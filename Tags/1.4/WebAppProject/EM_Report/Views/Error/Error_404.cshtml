@model System.Web.Mvc.HandleErrorInfo
@using EM_Report.Helpers;
@using EM_Report.Domain.Resources;

@{
    ViewBag.Title = "Error";    
    Layout = Request.IsAjaxRequest() ? null : (Base.LoginSession == null ? "~/Views/Shared/_MasterWithoutLogin.cshtml" : "~/Views/Shared/_Master.cshtml");
}
@functions
{
    public bool IsLogin()
    {
        return Base.LoginSession == null ? false : true;
    }
}

<div class="wrapper">
    <div class="container">
    	<div class="w_100p">
            @if (!IsLogin())
            {
                <div class="header"></div>
            }
            <div class="formWrap" style=" top:auto;">
            	<h1>404</h1>
                <hr /><br />
                <p>@Resource.msg404</p>
            </div>
        </div>
    </div>
</div>