@using EM_Report.Domain;
@using System.Web;
@{
    ViewBag.Title = "DashBoard";
    ViewBag.Heading = "Report >> Dashboard >> Level 0";
    Layout = Request.IsAjaxRequest() ? null : "~/Views/Shared/_Master.cshtml";
}
<div class="w_100p" style="margin: 0 !important;">    
    <div class="content">
        @{
            var url = "~/DashboardReports/DashboardReport.aspx?Systems=" + ViewBag.Systems + "&ReportPath=" 
                + HttpUtility.UrlEncode((string)(ViewBag.ReportPath));
        }
        <iframe id="ifReport" src="@Url.Content(@url)" width="100%" height="100%" frameborder="0" scrolling="no" style="display:inline">
        </iframe>
    </div>
    <div class="clear">
    </div>
    @Html.Partial("VisibilityTooltip")
</div>
@Html.Partial("TimeAccess")