@{
    ViewBag.Title = "DashBoard";
    ViewBag.Heading = "Report >> Dashboard >> Level 4";
    Layout = Request.IsAjaxRequest() ? null : "~/Views/Shared/_Master.cshtml";
}
<div class="w_100p" style="margin: 0 !important;">
    <div class="content">
        @Html.Partial("~/Views/Dashboard/CPRViewType.cshtml")
        @{
            string url;
            if (((string)(ViewBag.ReportPath)).ToLower().Contains("cpr"))
            {
                url = "~/DashboardReports/CPRDashboardReport.aspx";
            }
            else if (((string)(ViewBag.ReportPath)).ToLower().Contains("rtw"))
            {
                url = "~/DashboardReports/RTWDashboardReport.aspx";
            }
            else
            {
                url = "~/DashboardReports/AWCDashboardReport.aspx";
            }

            string strQuery = "";
            foreach (String key in Request.QueryString.AllKeys)
            {
                strQuery = strQuery + "&" + key + "=" + Request.QueryString[key];
            }
            strQuery = "?" + strQuery.TrimStart('&');
            url = url + strQuery;
        }
        <iframe id="ifReport" src="@Url.Content(@url)" width="100%" height="100%" frameborder="0" scrolling="no"></iframe>
    </div>
    <div class="clear"></div>
</div>

