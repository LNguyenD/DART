<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <title>@ViewBag.Title</title>
    <link rel="shortcut icon" href="@Url.Image("favicon.ico")" />
    <link href="@Url.Stylesheet("jquery/jquery.ui.all.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Stylesheet("style.css")" rel="stylesheet" type="text/css" />    
    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>      
    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.min.js")"></script>
    <script language="javascript" type="text/javascript" src="@Url.Content("~/Scripts/MicrosoftAjax.js")"></script>
    <script language="javascript" type="text/javascript" src="@Url.Content("~/Scripts/MicrosoftMvcAjax.js")"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.16.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.ui.dialog.js")" type="text/javascript"></script>    
    <script src="@Url.Content("~/Scripts/jquery.ui.draggable.js")" type="text/javascript"></script>       
    <script src="@Url.Content("~/scripts/jsBase.js")" type="text/javascript"></script>         
    <script type="text/javascript">
        window.g_baseUrl = '@Url.Content("~")';
        window.g_baseUrl = window.g_baseUrl.substring(0, window.g_baseUrl.length - 1);
</script>      
</head>
<body>
    <section>
        <header> <!--Reder header-->
            <div id="header">  
	            @Html.Partial("_MainMenu")                 
	        </div>
        </header>
        <article> <!--Render content -->
            <div id="dialog-confirm"></div>
            <div class="loading"><img src="@Url.Image("ajax-loader.gif")" alt="loading" width="200px" height="100%" /></div>
            <div id="content">
                @RenderBody()     
            </div>                   
        </article>
       <footer> <!--Render footer-->
            <div class="footer_div">
                @EM_Report.BLL.Resources.Resource.msgFooter
            </div>
        </footer>
    </section>
</body>
</html>
